{
  "name": "strikeshark",
  "description": "strikeshark framework Node.js w/ Express",
  "version": "0.0.1",
  "main": "bin/index.mjs",
  "author": "JhonArroyo",
  "license": "ISC",
  "scripts": {
    "start": "node bin/index.mjs",
    "dev": "node --trace-warnings bin/index.mjs",
    "new-strikeshark": "node libs/_createFile.mjs",
    "wbpckg:watch-strikeshark": "webpack --mode development --watch",
    "wbpckg:build-strikeshark": "webpack --mode production",
    "migrate:make-strikeshark": "npx knex --knexfile libs/_knexMigration.mjs migrate:make",
    "migrate:rollback-strikeshark": "npx knex --knexfile libs/_knexMigration.mjs migrate:rollback",
    "migrate:latest-strikeshark": "npx knex --knexfile libs/_knexMigration.mjs migrate:latest"
  },
  "dependencies": {
    "body-parser": "^1.20.2",
    "bootstrap": "^5.3.2",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "ejs": "^3.1.9",
    "express": "^4.18.1",
    "https": "^1.0.0",
    "knex": "^2.5.1",
    "mongoose": "^7.5.0",
    "mssql": "^10.0.1",
    "mysql": "^2.18.1",
    "serve-favicon": "^2.5.0",
    "url": "^0.11.1"
  },
  "devDependencies": {
    "css-loader": "^6.8.1",
    "mini-css-extract-plugin": "^2.7.6",
    "node-sass": "^9.0.0",
    "nodemon": "^3.0.1",
    "sass": "^1.66.1",
    "sass-loader": "^13.3.2",
    "style-loader": "^3.3.3",
    "webpack": "^5.88.2",
    "webpack-cli": "^5.1.4"
  }
}
